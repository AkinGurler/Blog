nst[editMode,setEditMode]=useState(false);\r\n  const openEditMode=()=>{\r\n    setEditMode(true);\r\n  };\r\n  const closeEditMode=()=>{\r\n    setEditMode(false);\r\n  }\r\n\r\n  useEffect(()=>{\r\n    \r\n    dispatch(fetchSinglePost(id))\r\n  },[dispatch])\r\n\r\n  const convertRelativeTime = (date) => {\r\n    return moment(date).fromNow();\r\n  };\r\n  \r\n  const removePost=()=>{\r\n    dispatch(deletePost(currentPost._id))\r\n    history.push(\"/posts\");\r\n  }\r\n\r\n  const classes=useStyles();\r\n\r\n  return (\r\n    <Paper className={classes.paper} elevation={0}>\r\n      {\r\n        editMode ? (\r\n          <EditPostForm post={currentPost} closeEditMode={closeEditMode} ></EditPostForm>\r\n        ) : (\r\n        <div>\r\n          <div className={classes.header}>\r\n            <Typography variant=\"h5\" gutterBottom>\r\n              {currentPost?.title}\r\n            </Typography>\r\n            {(user?.result?.sub===currentPost?.creator || user?.result?._id===currentPost?.creator) &&(\r\n            <div>\r\n              <Button\r\n                color=\"primary\"\r\n                variant=\"outlined\"\r\n                startIcon={<EditIcon />}\r\n                onClick={openEditMode}\r\n              >\r\n                Edit\r\n              </Button>{\" \"}\r\n              <Button\r\n                color=\"secondary\"\r\n                variant=\"outlined\"\r\n                onClick={removePost}\r\n                startIcon={<DeleteIcon />}\r\n              >\r\n               Delete\r\n              </Button>\r\n              </div>\r\n            )\r\n            }\r\n          </div>\r\n\r\n          <Divider />\r\n          <Typography variant=\"overline\" gutterBottom>\r\n            {currentPost?.subtitle}\r\n          </Typography>\r\n          <Typography variant=\"caption\" component=\"p\" gutterBottom>\r\n            {convertRelativeTime(currentPost?.createdAt)} by Akın\r\n          </Typography>\r\n          <Chip\r\n     